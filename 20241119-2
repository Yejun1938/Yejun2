#include <stdio.h>
int main()
{
	int i=1;
	while(i<=6)
	{
		printf("%d학년 ||", i);
		int j=1;
		for(j=7;j>=1;j--)
		{
			printf(" %d반 ", j);
		}
		printf("\n");
		i++;
	}
	
}

int main()
{
	for(int i=6; i>=1; i--)
	{
		printf(" %d학년 ||", i);
		int j=7;
		while(j>=1)
		{
			printf(" %d반 ", j);
			j--;
		}
		printf("\n");
		
	}
 } 

int main()
{
	for(int i=1;i<=5;i++)
	{
		for(int j=1;j<=i;j++)
		{
			printf("%d ", j);
		}
		printf("\n");
	}
}

int main()
{
	for(int i=5;i>=1;i--)
	{
		for(int j=1;j<=i;j++)
		{
			printf("%d ", j);
		}
		printf("\n");
	}
}

int main()
{
	for(int i=5;i>=1;i--)
	{
		for(int j=i;j<=5;j++)
		{
			printf("%d ", j);
		}
		printf("\n");
	}
}


int main()
{
	for(int i=5;i>=1;i--)
	{
		for(int j=i;j>=1;j--)
		{
			printf("%d ", j);
		}
		printf("\n");
	}
}

int main()
{
	for(int i=1;i<=5;i++)
	{
		for(int j=5-i;j>=1;j--)
		{
			printf("  ");
		}
		for(int j=i;j>=1;j--)
		{
			printf("%d ", j);
		}
		printf("\n");
	}
}
