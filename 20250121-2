#include <stdio.h>
#include <string.h>

struct Person
{
	char name[10];
	int age;
	double height;
 };
 
 int main()
 {
 	struct Person m1;
 	struct Person m2 = {"허난설헌", 13, 159.9};
 	
 	strcpy(m1.name, "허균");
	m1.age = 19;
	m1.height = 168.34; 
	
	struct Person m4;
	strcpy(m4.name, "김예준");
	m4.age = 15;
	m4.height = 168;
	printf("%s\n", m4.name);
	printf("%d\n", m4.age);
	printf("%f", m4.height);

 }
